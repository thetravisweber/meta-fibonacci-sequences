CC = gcc
DEBUG = -g
CFLAGS = -Wall -c $(DEBUG)
LFLAGS = -Wall $(DEBUG)

Test : SetUtils.o HofUtils.o test.o
	$(CC) $(LFLAGS) SetUtils.o HofUtils.o test.o -o Test

test.o : test.c SetUtils.h HofUtils.h
	$(CC) $(CFLAGS) test.c

SetUtils.o : SetUtils.c SetUtils.h
	$(CC) $(CFLAGS) SetUtils.c

HofUtils.o : HofUtils.c HofUtils.h
	$(CC) $(CFLAGS) HofUtils.c

clean:
	\rm *.o Test

